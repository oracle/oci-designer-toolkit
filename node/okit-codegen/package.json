    "main": "src/okit-codegen.js",
    "scripts": {
        "start": "node .",
        "test-generate-okit-model": "node src/okit-codegen.js generate okit-model-js ./src/code_generation/schema/tf-schema.json test/model",
        "test-generate-okit-properties": "node src/okit-codegen.js generate okit-properties-js ./src/code_generation/schema/tf-schema.json test/properties",
        "generate-okit-model": "node src/okit-codegen.js generate okit-model-js ./src/code_generation/schema/tf-schema.json ../okit-node/src/model",
        "generate-okit-properties": "node src/okit-codegen.js generate okit-properties-js ./src/code_generation/schema/tf-schema.json ../okit-node/src/properties"
    },
    "private": true,
    "type": "module",
    "dependencies": {
        "jsdom": "^16.5.1",
        "okit-node": "file:../okit-node"
    }
