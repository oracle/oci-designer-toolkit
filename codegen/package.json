{
    "name": "ocd-codegen",
    "version": "0.1.0",
    "description": "Open Cloud Designer Module Generator",
    "author": {
        "name": "Andrew Hopkinson",
        "email": "andrew.hopkinson@oracle.com",
        "url": "https://github.com/oracle/oci-designer-toolkit"
    },
    "bugs": "https://github.com/oracle/oci-designer-toolkit/issues",
    "homepage": "https://github.com/oracle/oci-designer-toolkit",
    "keywords": [
        "oci",
        "oracle cloud",
        "ocd",
        "ocd-codegen",
        "azure",
        "aws",
        "google"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/oracle/oci-designer-toolkit"
    },
    "main": "src/ocd-codegen.js",
    "scripts": {
        "start": "node .",
        "generate-oci-model": "node src/ocd-codegen.js generate oci-model-js ./src/schema/oci-schema.json ../ocd/src/model/provider/oci",
        "generate-oci-properties": "node src/ocd-codegen.js generate oci-properties-js ./src/schema/oci-schema.json ./test/ocd/src/properties/providers/oci",
        "generate-oci-ansible": "node src/ocd-codegen.js generate oci-ansible-js ./src/schema/oci-schema.json ./test/ocd/src/generators/providers/oci/ansible",
        "generate-oci-terraform": "node src/ocd-codegen.js generate oci-terraform-js ./src/schema/oci-schema.json ./test/ocd/src/generators/providers/oci/terraform",
        "generate-oci-markdown": "node src/ocd-codegen.js generate oci-markdown-js ./src/schema/oci-schema.json ./test/ocd/src/generators/providers/oci/markdown",
        "import-oci-terraform-schema": "node src/ocd-codegen.js import terraform-schema  ./src/schema/tf-schema.json ./src/schema/oci-schema.json"
    },
    "private": true,
    "type": "module",
    "dependencies": {
        "jsdom": "^16.5.1"
    },
    "license": "UPL-1.0",
    "devDependencies": {
        "@types/node": "^18.14.2",
        "typescript": "^4.9.5"
    }
}
